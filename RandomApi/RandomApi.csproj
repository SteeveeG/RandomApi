<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net8.0-windows</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <UseWPF>true</UseWPF>
    </PropertyGroup>
    <ItemGroup>
      <None Remove="Assets\Custom.ttf" />
      <Resource Include="Assets\Custom.ttf">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Resource>
      <None Remove="Assets\JacquardaBastarda9.ttf" />
      <Resource Include="Assets\JacquardaBastarda9.ttf">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Resource>
      <None Remove="Assets\InconsolataCondensed.ttf" />
      <Resource Include="Assets\InconsolataCondensed.ttf">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Resource>
    </ItemGroup>
    <ItemGroup>
      <PackageReference Include="NekosBestApi" Version="1.0.0" />
    </ItemGroup>
    <ItemGroup>
      <Compile Remove="Models\RandomDukModel.cs" />
    </ItemGroup>

</Project>
